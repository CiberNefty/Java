
public class Bucles {

	public static void main(String[] args) {
		
		/*/ BUCLE FOR: mostrar por consola los numeros del 1 al 100
		for(int x=0; x<= 100;x++) {
			System.out.println("El numero es: "+x);
		}*/
		
		//WHILE
		// Declaramos una variable y la iniciamos
		byte numero=1;
		while(numero <=100){
			System.out.println("El numero es: "+numero);
			
			numero++;
		}
		
	}

}
